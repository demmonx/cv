{% for media in medias %}
     {% if media.name == "linkedin" %}
         <li><a href="#" class="fa fa-linkedin"><span>Linkedin</span></a></li>

     {% elseif media.name == "instagram" %}
         <li><a href="{{ media.url }}" class="fa fa-instagram"><span>Instagram</span></a></li>

     {% elseif media.name == "facebook" %}
        <li><a href="{{ media.url }}" class="fa fa-facebook"><span>Facebook</span></a></li>

    {% elseif media.name == "twitter" %}
        <li><a href="{{ media.url }}" class="fa fa-twitter"><span>Twitter</span></a></li>

    {% elseif media.name == "youtube" %}
        <li><a href="{{ media.url }}" class="fa fa-youtube"><span>Youtube</span></a></li>

    {% elseif media.name == "stackoverflow" %}
        <li><a href="{{ media.url }}" class="fa fa-stack-overflow"><span>Stack Overflow</span></a></li>

    {% endif %}
{% endfor %}